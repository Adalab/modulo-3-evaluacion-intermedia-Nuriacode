  const handleFilter = (ev) => {\n    setSearch(ev.target.value);\n  };\n\n  const handleCharacter =(ev)=>{\n    setSelectCharacter(ev.target.value);\n    \n  }\n\n  return (\n    <div className=\"App\">\n      <main>\n        <h1>Frases de Friends</h1>\n        <h2>Listado de frases</h2>\n        <label>Filtra por frase</label>\n        <input\n          type=\"search\"\n          autoComplete=\"off\"\n          name=\"search\"\n          placeholder=\"Escribe aquí\"\n          onInput={handleFilter}\n          value={search}\n        />\n        <label>Filtra por personaje</label>\n        <select name='character' onChange={handleCharacter} value={selectCharacter}>\n          <option value='Todos'>Todos</option>\n          <option value='Ross'>Ross</option>\n          <option value='Monica'>Monica</option>\n          <option value='Joey'>Joey</option>\n          <option value='Phoebe'>Phoebe</option>\n          <option value='Chandler'>Chandler</option>\n          <option value='Rachel'>Rachel</option>\n        </select>\n        <ul>{renderListPhrases()}</ul>\n        <h2>Añadir una nueva frase</h2>\n\n        <from>\n        <label>Frase</label>\n        <input\n          type=\"text\"\n          autoComplete=\"off\"\n          name=\"phrase\"\n          placeholder=\"Escribe aquí\"\n        />\n        <label>Personaje</label>\n        <input\n          type=\"text\"\n          autoComplete=\"off\"\n          name=\"character\"\n          placeholder=\"Escribe aquí\"\n        />\n        <input\n          type=\"submit\"\n          autoComplete=\"off\"\n          name=\"search\"\n          value=\"Añadir nueva frase\"\n        />\n        </from>\n      </main>\n    </div>\n  );\n}\n\nexport default App;\n",{"ruleId":"15","severity":1,"message":"16","line":6,"column":10,"nodeType":"17","messageId":"18","endLine":6,"endColumn":17},{"ruleId":"15","severity":1,"message":"19","line":6,"column":19,"nodeType":"17","messageId":"18","endLine":6,"endColumn":29},"no-unus